#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
	FILE *pfile;
	if (argc != 2) {
		puts("Informe o nome do arquivo!");
		return EXIT_FAILURE;
	}
	p_file = fopen(argv[1], "r");

	return EXIT_SUCCESS;
}
